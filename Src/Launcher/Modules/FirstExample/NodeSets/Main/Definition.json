            "select": {
              "query": "SELECT * FROM students"
            }
          }
        },
        {
          "nodekey": "students",
          //"datapluginname": "SqLiteData",
          "executionorder": 0,
          "isList": 1,
          "read": {
            "select": {
              "file": "sql/select/selectStudents.sql"
            }
          },
          "write": {
            "insert": {
              "query": "sql/insert/insertStudent.sql"
            },
            "update": {
              "query": "sql/update/updateStudents.sql"
            },
            "delete": {
